-- main functions

function _init()
	
	-- declare variables
	
	gamestate = 0
	
	-- title 0
	-- gameplay 1
	-- win 2
	-- lose 3
	
	player = 
 {
 	x = 64,
 	y = 112,
 	vx = 1, -- velocity x
 	s = 5, -- sprite index
 	cx = 3, -- cannon offset x
 	cy = 3, -- cannon offset y
 	score = 0,
 	timer = 121 -- 2 minutes + 1 second for the start the game
 }
 
 projectiles = {}
 
 shields = {}
 init_shields()
 
 enemies = {}
 init_enemies()
 enemy_projectiles = {}
 
 frame = 0
 move_counter = 0
 
 bosses = {} 
 init_bosses()-- currently only one
 bosses_projectiles = {}
	
end

-- update states

function _update()

	if (gamestate == 0) then
	
		update_title()
		
	elseif (gamestate == 1) then
	
		update_gameplay()
		
	elseif (gamestate == 2) then
	
		update_win()
		
	elseif (gamestate == 3) then
	
		update_lose()
		
	end
	
	framecounter()
	
end

function update_title()

	press_key_logic()
	
end

function update_gameplay()

	player_timer_logic()
	player_move_logic()
	player_collision_logic()
	
	projectiles_shoot_logic()
	projectiles_move_logic()
	
	enemies_move_logic()
	enemies_shoot_logic()
	enemy_projectiles_move_logic()
	
	bosses_move_logic()
	bosses_collision_logic()
	bosses_shoot_logic()
	bosses_projectiles_move_logic()
	
end

function update_win()

	press_key_logic()
	
end

function update_lose()

	press_key_logic()

end

-- draw states

function _draw()

	if (gamestate == 0) then
	
		draw_title()
		
	elseif (gamestate == 1) then
	
		draw_gameplay()
		
	elseif (gamestate == 2) then
	
		draw_win()
		
	elseif (gamestate == 3) then
	
		draw_lose()
		
	end
	
end

function draw_title()

	cls()
	
	output_lettering()
	
	output_player_sprite()
	output_control()
	
	output_start_game_text()
	
end

function draw_gameplay()

	cls()
	
	output_line_outside()
	
	output_player()
	output_player_score()
	output_player_timer()
	
	output_projectiles()
	
	output_shields()
	
	output_enemies()
	output_enemy_projectiles()
	
	output_bosses()
	output_bosses_projectiles()
	
end

function draw_win()

	cls()
	
	output_win_text()
	
	output_press_key_text()
	
end

function draw_lose()

	cls()
	
	output_lose_text()
	
	output_press_key_text()
	
end

-- secondary functions

-- functions for _init()

function init_shields()

	-- declare variables
	
	number_shields_by_column = 11
	number_shields_by_line = 3
	
	for x = 10, number_shields_by_column * 10, 10 do
	
		for y = 5, number_shields_by_line * 5, 5 do
		
			add_shields
			(
				3 + x, -- position x origin + distance
				95 + y, -- position y origin + distance
				3 + x + 3, -- position x origin + distance + width
				95 + y -- position y origin + distance
			)
			
		end
		
	end

end

function init_enemies()

	-- declare variables
	
	number_enemies_third_by_line = 10
	number_enemies_second_by_line = 10
	number_enemies_first_by_line = 10
	
	number_lines_for_enemies = 1

 -- third enemies
 
	for y = 8, number_lines_for_enemies * 8, 8 do
	
		for x = 10, number_enemies_third_by_line * 10, 10 do
			
			add_enemies_third
			(
				6 + x, -- position x + sprite
				30 + y -- position y + sprite
			)
			
		end
		
	end
	
	-- second enemies
	
	for y = 8, number_lines_for_enemies * 8, 8 do
	
		for x = 10, number_enemies_second_by_line * 10, 10 do
			
			add_enemies_second
			(
				6 + x, -- position x + sprite
				40 + y -- position y + sprite
			)
			
		end
		
	end
	
		-- first enemies
		
	for y = 8, number_lines_for_enemies * 8, 8 do
	
		for x = 10, number_enemies_first_by_line * 10, 10 do
			
			add_enemies_first
			(
				6 + x, -- position x + sprite
				50 + y -- position y + sprite
			)
			
		end
		
	end
		
end

function init_bosses()
	
		add_bosses
		(
			63,
			63
		)

end

-- functions for update_title(),
-- update_win() and update_lose()

function press_key_logic()

	-- if the key "s" pressed, 
	-- then the game is started.
	
	if (btn(0, 1)) then
	
		_init()
		
		gamestate = 1
		
	end
	
end

-- functions for update_gameplay()

function player_timer_logic()

	-- after one second, the 
	-- timer is decremented.
	
	if (frame % 30 == 0) then
	
		player.timer -= 1
		
	end
	
	-- when the time is up, then 
	-- the player has lost.
	
	-- -1 so that the player sees 
	-- the 0
	
	if (player.timer == -1) then
	
		gamestate = 3
		
	end

end

function player_move_logic()
	
	-- left button
	
	if (btn(0)) then
	
		player.x -= player.vx
		
	-- right button
		
	elseif (btn(1)) then 
	
		player.x += player.vx
		
	end

end

function player_collision_logic()

	if (player.x <= 0) then
	
		player.x = 0
		
	-- 6 is the width of the sprite
	
	elseif (player.x + 6 >= 127) then
	
		player.x = 127 - 6
		
	end

end

function projectiles_shoot_logic()
	
	-- press button x to shoot 
	
	if (btnp(5)) then
	
		add_projectiles
		(
			player.x + player.cx,
			player.y + player.cy
		)
		
	end

end

function projectiles_move_logic()

	for laser in all(projectiles) do
		
		laser.y -= laser.vy
			
		-- delete projectiles 
		-- to prevent out of memory
			
		delete_projectiles(projectiles, laser)
			
		-- delete shields
		-- by collision
			
		delete_shields(projectiles, laser)
			
		-- delete enemies
		-- by collision
			
		delete_enemies(projectiles, laser)
		
		-- delete boss
		-- by collision
		
		if (#enemies == 0) then
		
			for final_boss in all(bosses) do
		
				if (laser.x >= final_boss.x and
						 	laser.x <= final_boss.x + 8 and
						 	laser.y >= final_boss.y and
						 	laser.y <= final_boss.y + 8) then
						
					gamestate = 2
				
				end
				
			end
			
		end
			
	end

end

function enemies_move_logic()

 -- increment move_counter
			
	if (frame % 30 == 0) then
			
		move_counter += 1
				
	end
	
	-- enemies move
			
	-- up
			
	if (move_counter == 2) then
			
		for creature in all(enemies) do
				
			creature.y -= 0.4
				
		end
				
	-- middle
				
	elseif (move_counter == 4) then
			
		for creature in all(enemies) do
			
		 creature.y += 0.4
		 
		end
				
	-- right
				
	elseif (move_counter == 6) then
			
			for creature in all(enemies) do
			
				creature.x += 0.4
				
			end
				
	-- middle
				
	elseif (move_counter == 8) then
		
		for creature in all(enemies) do
		
			creature.x -= 0.4
		
		end
				
	-- down
				
	elseif (move_counter == 10) then
		
		for creature in all(enemies) do
		
				creature.y += 0.4
				
		end
				
	-- middle
				
 elseif (move_counter == 12) then
		
		for creature in all(enemies) do
		
			creature.y -= 0.4
		
		end
				
	-- left
				
	elseif (move_counter == 14) then
		
		for creature in all(enemies) do
			
				creature.x -= 0.4
				
		end
				
	-- middle
				
	elseif (move_counter == 16) then
			
		for creature in all(enemies) do
		
			creature.x += 0.4
			
		end
				
	-- reset counter
				
	elseif (move_counter == 18) then
			
		move_counter = 0	
				
	end
				
end

function enemies_shoot_logic()

	-- declare variable
	
	random_shoot = 0

	for creature in all(enemies) do
	
		random_shoot = flr(rnd(1000))
		
		if (random_shoot == 1) then
		
			add_enemy_projectiles
			(
				creature.x + 4, -- cannon offset 4
				creature.y + 4 -- cannon offset 4
			)
			
		end
		
	end

end

function enemy_projectiles_move_logic()

	for laser in all(enemy_projectiles) do
		
		laser.y += laser.vy
			
		-- delete projectiles 
		-- to prevent out of memory
		
		delete_enemy_projectiles(enemy_projectiles, laser)
			
		-- delete shields
		-- by collision
			
		delete_shields(enemy_projectiles, laser)
		
		-- if the player is hit by 
		-- the enemy laser, then he 
		-- has lost.
		
		-- player width = 5
		-- player height = 4
		
		if (laser.x >= player.x and
						laser.x <= player.x + 5 and
						laser.y >= player.y and
						laser.y <= player.y + 4) then
						
						gamestate = 3
				
		end
		
	end

end

function bosses_move_logic()

	if (#enemies == 0) then

		-- increment move_counter
			
		if (frame % 30 == 0) then
			
			move_counter += 1
				
		end
	
		-- boss move
			
		-- up
			
		if (move_counter == 2) then
			
			for final_boss in all(bosses) do
				
				final_boss.y -= rnd(4)
				
			end
		
		-- down
		
		elseif (move_counter == 4) then
			
			for final_boss in all(bosses) do
				
				final_boss.y += rnd(3)
				
			end
		
		-- right
		
		elseif (move_counter == 6) then
			
			for final_boss in all(bosses) do
				
				final_boss.x += rnd(3)
				
			end
		
		-- left
		
		elseif (move_counter == 8) then
			
			for final_boss in all(bosses) do
				
				final_boss.x -= rnd(5)
				
			end
		
		end
	
		-- reset counter
	
		if (move_counter > 8) then
	
			move_counter = 0
		
		end
		
	end

end

function bosses_collision_logic()

	if (#enemies == 0) then
	
		for final_boss in all(bosses) do
	
			if (final_boss.x >= 119 or
							final_boss.x <= 8 or
							final_boss.y <= 23 or
							final_boss.y >= 87) then
						
				delete_bosses(bosses, final_boss)
						
				init_bosses()
			
			end
		
		end
		
	end

end

function bosses_shoot_logic()

		-- declare variable
	
	random_shoot = 0

	if (#enemies == 0) then
	
		for final_boss in all(bosses) do
	
			random_shoot = flr(rnd(10))
		
			if (random_shoot == 1) then
		
				add_bosses_projectiles
				(
					final_boss.x + 4, -- cannon offset 4
					final_boss.y + 4 -- cannon offset 4
				)
			
			end
		
		end
		
	end

end

function bosses_projectiles_move_logic()

	if (#enemies == 0) then
	
		for laser in all(bosses_projectiles) do
		
			laser.y += laser.vy
			
			-- delete projectiles 
			-- to prevent out of memory
		
			delete_bosses_projectiles(boss_projectiles, laser)
			
			-- delete shields
			-- by collision
			
			delete_shields(bosses_projectiles, laser)
		
			-- if the player is hit by 
			-- the enemy laser, then he 
			-- has lost.
		
			-- player width = 5
			-- player height = 4
		
			if (laser.x >= player.x and
							laser.x <= player.x + 5 and
							laser.y >= player.y and
							laser.y <= player.y + 4) then
							
							gamestate = 3
				
			end
		
		end
		
	end

end

