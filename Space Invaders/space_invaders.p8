-- functions for add objects

function add_projectiles(_x, _y)
	
	-- declare variables
	
	local p =
	{
		x = _x,
		y = _y,
		vy = 1, -- velocity y
		width = 1,
		height = 3,
		colour = 11 -- green
	}
	
	add(projectiles, p)
	
end

function add_shields(_x, _y, _width, _height)
	
	-- declare variables
	
	local s =
	{
		x = _x,
		y = _y,
		width = _width,
		height = _height,
		colour = 1 -- dark blue
	}
	
	add(shields, s)
	
end

-- enemies

function add_enemies_first(_x, _y)

	local e = 
	{
		x = _x,
		y = _y,
		s = 1 -- sprite index
	}
	
	add(enemies, e)

end

function add_enemies_second(_x, _y)

	local e = 
	{
		x = _x,
		y = _y,
		s = 2 -- sprite index
	}
	
	add(enemies, e)

end

function add_enemies_third(_x, _y)

	local e = 
	{
		x = _x,
		y = _y,
		s = 3 -- sprite index
	}
	
	add(enemies, e)

end

function add_bosses(_x, _y)

	local b = 
	{
		x = _x,
		y = _y,
		s = 4 -- sprite index
	}
	
	add(bosses, b)

end

function add_enemy_projectiles(_x, _y)
	
	-- declare variables
	
	local p =
	{
		x = _x,
		y = _y,
		vy = 1, -- velocity y
		width = -1, -- for perpendicular line
		height = 3,
		colour = 4 -- red
	}
	
	add(enemy_projectiles, p)
	
end

function add_bosses_projectiles(_x, _y)
	
	-- declare variable
	
	local p =
	{
		x = _x,
		y = _y,
		vy = 1, -- velocity y
		width = -1, -- for perpendicular line
		height = 3,
		colour = 4 -- red
	}
	
	add(bosses_projectiles, p)
	
end

-- functions for delete objects

function delete_projectiles(_projectiles, _laser)

		-- crossing_up + laser.height 
		-- = 18
		
		if (_laser.y + _laser.height <= 18) then
		
			del(_projectiles, _laser)
			
		end
		
end

function delete_shields(_projectiles, _laser)

	for plate in all(shields) do
		
		if (_laser.x >= plate.x and
						_laser.x <= plate.x + 3 and
						_laser.y >= plate.y and
						_laser.y <= plate.y + 1) then
						
			del(_projectiles, _laser)
			del(shields, plate)
				
		end
			
	end
	
end

function delete_enemies(_projectiles, _laser)

	for creature in all(enemies) do
		
		if (_laser.x >= creature.x and
						_laser.x <= creature.x + 8 and
						_laser.y >= creature.y and
						_laser.y <= creature.y + 8) then
						
			del(_projectiles, _laser)
			del(enemies, creature)
			
			player.score += 10
				
		end
			
	end

end

function delete_enemy_projectiles(_enemy_projectiles, _laser)
		
		if (_laser.y + _laser.height >= 127) then
		
			del(_enemy_projectiles, _laser)
			
		end
		
end

function delete_bosses_projectiles(_boss_projectiles, _laser)

		if (_laser.y + _laser.height >= 127) then
		
			del(_bosses_projectiles, _laser)
			
		end

end

function delete_bosses(_bosses, _laser)

	for final_boss in all(bosses) do
		
		if (_laser.x >= final_boss.x and
						_laser.x <= final_boss.x + 8 and
						_laser.y >= final_boss.y and
						_laser.y <= final_boss.y + 8) then
						
			del(bosses, final_boss)
				
		end
			
	end

end

-- other auxiliary functions

function framecounter()

	frame += 1
	
	if (frame == 30) then
	
		frame = 0
		
	end

end
