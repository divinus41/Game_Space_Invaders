-- functions for draw_gameplay()

function output_line_outside()
	
	-- declare variables
	
	min_line = 0
	crossing_up = 15
	max_line = 127
	colour = 11 -- green
	
	line -- up
	(
		min_line, 
		crossing_up, 
		max_line, 
		crossing_up, 
		colour
	)
	
	line -- left
	(
		min_line, 
		crossing_up, 
		min_line, 
		max_line, 
		colour
	)
	 
	line -- right
	(
		max_line, 
		crossing_up, 
		max_line, 
		max_line, 
		colour
	)
	
	line -- down
	(
		min_line, 
		max_line, 
		max_line, 
		max_line, 
		colour
	)

end

function output_player()

 spr
 (
 	player.s,
 	player.x,
 	player.y
 )

end

